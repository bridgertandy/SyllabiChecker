{"version":"0.3.0","body":"\nfunction main(workbook: ExcelScript.Workbook) {\n    const worksheets = workbook.getWorksheets();\n    let subjects: string[] = [\"AICC\",\"AGRI\",\"BIOL\",\"CHEM\",\"CNST\",\"CPSC\",\"CSEC\",\"CSSP\",\"ENVI\",\"FERM\",\"GEOL\",\n                              \"GEOS\",\"HRTC\",\"MATH\",\"MMBS\",\"NADM\",\"CSSA\",\"PHYS\",\"SCIE\",\"SWDV\"];\n\n    for (const sheet of worksheets) {\n      const cellRange = sheet.getRange(\"B:B\").getUsedRange();\n      const columnValues = cellRange.getValues();\n      const startRowIndex = cellRange.getRowIndex();\n\n      for (let i = 0; i < columnValues.length; i++) {\n        const cellValue = columnValues[i][0] as string;\n        \n        if (subjects.includes(cellValue)) { \n          const rowIndex = startRowIndex +i;\n          const cellG = sheet.getRangeByIndexes(rowIndex,6,1, 1);         \n          cellG.getFormat().getFill().setColor(\"#E8E8E8\");\n      }\n      }\n    }\n  \n}","description":"","noCodeMetadata":null,"parameterInfo":"{\"version\":1,\"originalParameterOrder\":[],\"parameterSchema\":{\"type\":\"object\",\"default\":{},\"x-ms-visibility\":\"internal\"},\"returnSchema\":{\"type\":\"object\",\"properties\":{}},\"signature\":{\"comment\":\"\",\"parameters\":[{\"name\":\"workbook\",\"comment\":\"\"}]}}","apiInfo":"{\"variant\":\"synchronous\",\"variantVersion\":2}"}