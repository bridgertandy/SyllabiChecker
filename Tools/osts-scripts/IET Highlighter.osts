{"version":"0.3.0","body":"\nfunction main(workbook: ExcelScript.Workbook) {\n    const worksheets = workbook.getWorksheets();\n    let subjects: string[] = [\"AMET\", \"ATBD\", \"AUTO\", \"DRFT\", \"ENGR\", \"HTEC\", \"MACH\", \"PSER\",\"TTEC\", \"UAS\", \"WEMF\", \"WSCT\"];\n\n    for (const sheet of worksheets) {\n        const cellRange = sheet.getRange(\"B:B\").getUsedRange();\n        const columnValues = cellRange.getValues();\n        const startRowIndex = cellRange.getRowIndex();\n\n        for (let i = 0; i < columnValues.length; i++) {\n            const cellValue = columnValues[i][0] as string;\n\n            if (subjects.includes(cellValue)) {\n                const rowIndex = startRowIndex + i;\n                const cellG = sheet.getRangeByIndexes(rowIndex, 1, 1, 6);\n                cellG.getFormat().getFill().setColor(\"#FFE3B9\");\n            }\n        }\n    }\n\n}","description":"","noCodeMetadata":"","parameterInfo":"{\"version\":1,\"originalParameterOrder\":[],\"parameterSchema\":{\"type\":\"object\",\"default\":{},\"x-ms-visibility\":\"internal\"},\"returnSchema\":{\"type\":\"object\",\"properties\":{}},\"signature\":{\"comment\":\"\",\"parameters\":[{\"name\":\"workbook\",\"comment\":\"\"}]}}","apiInfo":"{\"variant\":\"synchronous\",\"variantVersion\":2}"}